{
  "name": "rs-manager",
  "description": "Manage Rocksmith playlists, songs, stats and dlc's",
  "author": "SandiC",
  "version": "0.6.4",
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "aes-js": "^3.1.1",
    "autoprefixer": "7.1.6",
    "await-spawn": "^2.1.1",
    "babel-eslint": "^8.2.5",
    "binary-parser": "^1.3.2",
    "chalk": "1.1.3",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "electron-is-dev": "^0.3.0",
    "electron-prompt": "^1.0.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.2",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-table-next": "^0.1.13",
    "react-bootstrap-table2-filter": "^0.3.0",
    "react-bootstrap-table2-paginator": "^0.1.5",
    "react-collapsible": "^2.2.0",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.1",
    "react-tooltip": "^3.6.1",
    "request-promise": "^4.2.2",
    "resolve": "1.6.0",
    "sqlite": "^2.9.2",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "tough-cookie": "^2.4.3",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "electron": "electron .",
    "electron-start": "node src/electron-wait-react",
    "react-start": "node scripts/start.js",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "postinstall": "electron-rebuild  -f -w  sqlite3",
    "package-mac": "electron-packager . 'Rocksmith Manager' --overwrite --platform=darwin --arch=x64 --icon=src/assets/icons/mac/mac.icns --prune=true --out=release-builds --ignore=config.dev.json --ignore=rsdb.dev.sqlite",
    "package-win": "electron-packager . \"Rocksmith Manager\" --overwrite --platform=win32 --arch=x64 --icon=src/assets/icons/win/rs.ico --prune=true --out=release-builds --ignore=config.dev.json --ignore=rsdb.dev.sqlite",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.4.0",
    "babel-core": "^6.26.0",
    "babel-jest": "20.0.3",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "cross-env": "^5.2.0",
    "devtron": "^1.4.0",
    "electron": "^2.0.3",
    "electron-builder": "^20.19.2",
    "electron-packager": "^12.1.0",
    "electron-rebuild": "^1.8.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "build": {
    "appId": "com.sandi.rsmanager",
    "win": {
      "icon": "./src/assets/icon.jpg"
    },
    "directories": {
      "buildResources": "public"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": "github:sandiz/rs-manager"
}
